{
  "name": "Hello World",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["cookies", "activeTab", "storage", "tabs"],
  "host_permissions": ["<all_urls>"],
  "description": "My first Chrome extension.",
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["popup/config.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
